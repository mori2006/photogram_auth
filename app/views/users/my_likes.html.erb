<div class="page-header">
  <h3>User #<%= @user.id %>'s likes</h3>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Caption</th>
        <th>Image</th>
      </tr>

      <% @likes.each do |like| %>
      <tr>
        <td><%= like.photo.caption %></td>
        <td><img src = "<%= like.photo.image %>"</td>

      </tr>
      <% end %>
    </table>
  </div>
</div>
